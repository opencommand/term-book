{
    "swagger": "2.0",
    "info": {
        "description": "TermBook 的后端接口文档",
        "title": "TermBook API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/document/list": {
            "get": {
                "description": "获取所有可打开的文档",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Document"
                ],
                "summary": "获取所有文档文件名",
                "responses": {
                    "200": {
                        "description": "查询成功返回文档名数组",
                        "schema": {
                            "$ref": "#/definitions/api.ListDocumentResponse"
                        }
                    },
                    "500": {
                        "description": "目录遍历失败",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    }
                }
            }
        },
        "/document/new": {
            "post": {
                "description": "创建一个空文档并保存",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Document"
                ],
                "summary": "新建文档",
                "parameters": [
                    {
                        "description": "新建文档请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.NewDocumentRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseNewDocument"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    }
                }
            }
        },
        "/document/open": {
            "post": {
                "description": "从文件中读取文档内容",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Document"
                ],
                "summary": "打开文档",
                "parameters": [
                    {
                        "description": "文档文件名",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.OpenDocumentRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseOpenDocument"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    }
                }
            }
        },
        "/document/save": {
            "post": {
                "description": "将当前文档保存到文件中",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Document"
                ],
                "summary": "保存文档",
                "parameters": [
                    {
                        "description": "要保存的文档内容",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.SaveDocumentRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    }
                }
            }
        },
        "/run-cell": {
            "post": {
                "description": "执行输入的命令并返回输出结果",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Cell"
                ],
                "summary": "执行单元格命令",
                "parameters": [
                    {
                        "description": "命令输入",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.RunCellRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseRunCell"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/api.ResponseBad"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "api.ListDocumentResponse": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "api.NewDocumentRequest": {
            "type": "object",
            "properties": {
                "filename": {
                    "type": "string"
                }
            }
        },
        "api.OpenDocumentRequest": {
            "type": "object",
            "properties": {
                "filename": {
                    "type": "string"
                }
            }
        },
        "api.ResponseBad": {
            "type": "object",
            "properties": {
                "msg": {
                    "type": "string"
                }
            }
        },
        "api.ResponseNewDocument": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/model.Document"
                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "api.ResponseOpenDocument": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/model.Document"
                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "api.ResponseRunCell": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/api.RunCellResponse"
                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "api.RunCellRequest": {
            "type": "object",
            "properties": {
                "input": {
                    "type": "string",
                    "example": "echo Hello World"
                }
            }
        },
        "api.RunCellResponse": {
            "type": "object",
            "properties": {
                "exec_time": {
                    "type": "string",
                    "example": "150ms"
                },
                "output": {
                    "type": "string"
                },
                "output_time": {
                    "type": "string",
                    "example": "2025-06-09T13:11:45.3390718+08:00"
                }
            }
        },
        "api.SaveDocumentRequest": {
            "type": "object",
            "properties": {
                "author": {
                    "type": "string"
                },
                "cells": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.Cell"
                    }
                },
                "filename": {
                    "type": "string"
                }
            }
        },
        "model.Cell": {
            "type": "object",
            "properties": {
                "exec_time": {
                    "type": "string"
                },
                "input": {
                    "type": "string"
                },
                "input_time": {
                    "type": "string"
                },
                "output": {
                    "type": "string"
                },
                "output_time": {
                    "type": "string"
                }
            }
        },
        "model.Document": {
            "type": "object",
            "properties": {
                "author": {
                    "type": "string"
                },
                "cells": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.Cell"
                    }
                },
                "filename": {
                    "type": "string"
                }
            }
        }
    }
}